<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp" >

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/tv_jbcs"
                    android:textColor="@color/base_color_text_black"
                    android:textSize="@dimen/textSize_svprogresshuddefault_msg" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/textView1"
                    android:text="@string/tv_jbcs_tips"
                    android:textColor="@color/base_color_text_black"
                    android:textSize="10sp" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/base_color_text_gray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <RelativeLayout
                        android:layout_width="96dp"
                        android:layout_height="match_parent" >

                        <TextView
                            android:id="@+id/t1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginTop="4dp"
                            android:text="@string/tv_spbt"
                            android:textColor="@color/base_color_text_black"
                            android:textSize="@dimen/textSize_svprogresshuddefault_msg" />
                    </RelativeLayout>

                    <EditText
                        android:id="@+id/edt_title"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/editbox_background_normal"
                        android:ems="10"
                        android:gravity="top"
                        android:hint="@string/tv_sixty"
                        android:maxLines="10"
                        android:minLines="8"
                        android:textColor="@color/base_color_text_black"
                        android:textColorHint="@color/black_text_color"
                        android:textSize="@dimen/text_mid_size" >

                        <requestFocus />
                    </EditText>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="4dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/img_hight_size" >

                    <RelativeLayout
                        android:layout_width="96dp"
                        android:layout_height="match_parent" >

                        <TextView
                            android:id="@+id/t5234"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="@string/tv_keys"
                            android:textColor="@color/base_color_text_black"
                            android:textSize="@dimen/textSize_svprogresshuddefault_msg" />
                    </RelativeLayout>

                    <EditText
                        android:id="@+id/edt_gjz"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/actionbar_hight"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:background="@drawable/editbox_background_normal"
                        android:ems="10"
                        android:gravity="top"
                        android:hint="@string/tv_thirty"
                        android:maxLines="3"
                        android:minLines="5"
                        android:textColor="@color/base_color_text_black"
                        android:textColorHint="@color/black_text_color"
                        android:textSize="@dimen/text_mid_size" >
                    </EditText>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/img_hight_size" >

                    <RelativeLayout
                        android:layout_width="96dp"
                        android:layout_height="match_parent" >

                        <TextView
                            android:id="@+id/t3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="@string/tv_zyyt"
                            android:textColor="@color/base_color_text_black"
                            android:textSize="@dimen/textSize_svprogresshuddefault_msg" />
                    </RelativeLayout>

                    <EditText
                        android:id="@+id/edt_yt"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/actionbar_hight"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:background="@drawable/editbox_background_normal"
                        android:ems="10"
                        android:gravity="top"
                        android:hint="@string/tv_thirty"
                        android:maxLines="3"
                        android:minLines="5"
                        android:textColor="@color/base_color_text_black"
                        android:textColorHint="@color/black_text_color"
                        android:textSize="@dimen/text_mid_size" >
                    </EditText>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/img_hight_size" >

                    <RelativeLayout
                        android:layout_width="96dp"
                        android:layout_height="match_parent" >

                        <TextView
                            android:id="@+id/t52"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="@string/tv_zycf"
                            android:textColor="@color/base_color_text_black"
                            android:textSize="@dimen/textSize_svprogresshuddefault_msg" />
                    </RelativeLayout>

                    <EditText
                        android:id="@+id/edt_cf"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/actionbar_hight"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:background="@drawable/editbox_background_normal"
                        android:ems="10"
                        android:gravity="top"
                        android:hint="@string/tv_thirty"
                        android:maxLines="3"
                        android:minLines="5"
                        android:textColor="@color/base_color_text_black"
                        android:textColorHint="@color/black_text_color"
                        android:textSize="@dimen/text_mid_size" >
                    </EditText>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/img_hight_size" >

                    <RelativeLayout
                        android:layout_width="96dp"
                        android:layout_height="match_parent" >

                        <TextView
                            android:id="@+id/t521"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="@string/tv_pp"
                            android:textColor="@color/base_color_text_black"
                            android:textSize="@dimen/textSize_svprogresshuddefault_msg" />
                    </RelativeLayout>

                    <EditText
                        android:id="@+id/edt_pp"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/actionbar_hight"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:background="@drawable/editbox_background_normal"
                        android:ems="10"
                        android:gravity="top"
                        android:hint="@string/tv_thirty"
                        android:maxLines="3"
                        android:minLines="5"
                        android:textColor="@color/base_color_text_black"
                        android:textColorHint="@color/black_text_color"
                        android:textSize="@dimen/text_mid_size" >
                    </EditText>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/img_hight_size" >

                    <RelativeLayout
                        android:layout_width="96dp"
                        android:layout_height="match_parent" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="@string/tv_dw"
                            android:textColor="@color/base_color_text_black"
                            android:textSize="@dimen/textSize_svprogresshuddefault_msg" />
                    </RelativeLayout>

                    <EditText
                        android:id="@+id/edt_dw"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/actionbar_hight"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:background="@drawable/editbox_background_normal"
                        android:ems="10"
                        android:textColor="@color/base_color_text_black"
                        android:textColorHint="@color/black_text_color"
                        android:textSize="@dimen/text_mid_size" >
                    </EditText>
                </LinearLayout>
            </LinearLayout>
<LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/img_hight_size" >

                    <RelativeLayout
                        android:layout_width="96dp"
                        android:layout_height="match_parent" >

                        <TextView
                            android:id="@+id/t5211"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="@string/tv_spjg"
                            android:textColor="@color/base_color_text_black"
                            android:textSize="@dimen/textSize_svprogresshuddefault_msg" />
                    </RelativeLayout>

                    <EditText
                        android:id="@+id/edt_jg"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/actionbar_hight"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:background="@drawable/editbox_background_normal"
                        android:ems="10"
                        android:inputType="number"
                        android:textColor="@color/base_color_text_black"
                        android:textColorHint="@color/black_text_color"
                        android:textSize="@dimen/text_mid_size" >
                    </EditText>
                </LinearLayout>
            </LinearLayout>
            <include layout="@layout/layout_rele_pro_img" />

            <Button
                android:id="@+id/btn_next"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:layout_margin="25dp"
                android:background="@drawable/button_bg"
                android:enabled="true"
                android:text="@string/tv_finish"
                android:textColor="@color/white_color"
                android:textSize="@dimen/textSize_svprogresshuddefault_msg" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>